{
  "name": "@vibe/monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo for vibe subscription manager",
  "scripts": {
    "setup:env": "node setup-env.js",
    "dev": "concurrently \"pnpm --filter @vibe/backend dev\" \"pnpm --filter @vibe/frontend dev\"",
    "build": "pnpm --filter \"./apps/*\" build",
    "test": "pnpm --filter \"./apps/*\" test",
    "lint": "pnpm --filter \"./apps/*\" lint",
    "backend:dev": "pnpm --filter @vibe/backend dev",
    "backend:build": "pnpm --filter @vibe/backend build",
    "backend:start": "pnpm --filter @vibe/backend start",
    "backend:db:generate": "pnpm --filter @vibe/backend db:generate",
    "backend:db:migrate": "pnpm --filter @vibe/backend db:migrate",
    "backend:db:studio": "pnpm --filter @vibe/backend db:studio",
    "backend:type-check": "pnpm --filter @vibe/backend type-check",
    "frontend:dev": "pnpm --filter @vibe/frontend dev",
    "frontend:build": "pnpm --filter @vibe/frontend build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "patchedDependencies": {},
    "hoist": [
      "**"
    ],
    "public-hoist-pattern": [
      "**"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}